  "engines": {
    "node": "16.x",
    "npm": ">=8.1.2"
  },
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm:dev:*\"",
    "dev:watch": "doppler run -- ts-node-dev --no-notify --respawn --transpile-only -r tsconfig-paths/register ./src",
    "build": "tsc && tscpaths -p ./tsconfig.json -s ./src -o ./.dist",
    "format": "prettier --write '**/*.{ts,tsx,yml,yaml}'",
    "lint": "eslint . --ext .ts --max-warnings=0 --fix",
    "db:schema:push": "doppler run -- npx prisma db push"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
