    "@fastify/multipart": "^7.1.1",
    "@fastify/rate-limit": "^7.3.0",
    "@prisma/client": "^4.4.0",
    "@sentry/node": "^7.11.1",
    "@sentry/tracing": "^7.11.1",
    "@zerollup/ts-transform-paths": "^1.7.18",
    "base64-js": "^1.5.1",
    "fastify": "^4.5.3",
    "fastify-file-upload": "^4.0.0",
    "http": "^0.0.1-security",
    "jsonwebtoken": "^8.5.1",
